------STRING FUNCTION

select 'my name is shreyansh tyagi'
select upper('my name is shreyansh tyagi');
select 'HEY EVERYONE'
select lower('HEY EVERYONE')
select reverse('HEY EVERYONE')
select ('     shreyansh')
select ltrim('     shreyansh')
select ('this is mysql workbench')
select substring('this is mysql workbench',9,11)
